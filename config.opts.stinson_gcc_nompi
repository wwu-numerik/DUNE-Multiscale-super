CONFIGURE_FLAGS="CXX=g++-4.8 CC=gcc-4.8 CXXFLAGS=' -DDEBUG -g3 -ggdb -std=c++0x -O0 -fopenmp' \
  --enable-fieldvector-size-is-method \
  --with-alugrid=/opt/dune/modules/alugrid/alugrid-1.52 \
  --with-alberta=/opt/dune/modules/alberta/alberta-3.0 \
  --enable-dunedevel \
  --enable-parallel \
  --disable-documentation  INKSCAPE=no"
CMAKE_FLAGS="-Wno-dev -DENABLE_MPI=0 -DCMAKE_BUILD_TYPE=DEBUG"
MAKE_FLAGS="-j8 -l8"
BUILDDIR=build-gcc-debug-nompi
